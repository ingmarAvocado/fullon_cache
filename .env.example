# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_SSL=false
REDIS_POOL_MIN_SIZE=1
REDIS_POOL_MAX_SIZE=10

# Cache TTL Settings (in seconds)
CACHE_TTL_DEFAULT=3600
CACHE_TTL_PROCESS=86400
CACHE_TTL_TICKER=300
CACHE_TTL_ACCOUNT=3600
CACHE_TTL_EXCHANGE=86400
CACHE_TTL_SYMBOL=3600
CACHE_TTL_BOT=3600
CACHE_TTL_ORDER_COMPLETED=86400
CACHE_TTL_TRADE_COMPLETED=86400

# OHLCV Settings
OHLCV_MAX_BARS_PER_KEY=10000

# Feature Flags
ENABLE_CACHE_METRICS=true
ENABLE_CACHE_COMPRESSION=false

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Performance Settings
MAX_CONNECTIONS=100
CONNECTION_TIMEOUT=10
SOCKET_TIMEOUT=5
SOCKET_KEEPALIVE=true
SOCKET_KEEPALIVE_OPTIONS=